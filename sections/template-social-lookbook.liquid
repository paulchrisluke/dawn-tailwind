<style>
  .page-width.page-content{
    width:100%;
  }
  .main-content .social-lookbook-section h2{
    margin-bottom:75px;
  }
  .social-lookbook-section h2 a{
    cursor:default;
    border-bottom:none !important;
  }
  .social-lookbook-section h2 a:after{
    display:none !important;
  }
  .main-content .social-lookbook-section{
    padding:0px;
  }
  .social-lookbook-section .sub-title-wrapper{
    display:none;
  }
</style>
<script>
  let slb_config = {
    show_all:true
  }
</script>
<div class="page-width page-content xmax-w-6xl">
  {%- render 'breadcrumbs' -%}
</div>
<!-- <div class="grid"> -->
<!--
  <div class="grid__item medium-up--three-quarters medium-up--push-one-eighth">
-->

<!-- Sticky Nav -->

<div class="social-lookbook-section !xoverflow-y-auto sm:xoverflow-y-visible">
  <h2 class="xhidden sm:xblock">
    <a href="/pages/our-social-lookbook-gumballpoodle">
      {{- page.title -}}
    </a>
  </h2>

  <div class="sub-title-wrapper">
    <span class="sub-title involvement-info">Get featured on our homepage</span>
  </div>

  <div class="social-lookbook-galley-wrapper">
    <div class="social-lookbook-galley">
      <div class="social-lookbook-media-container xflex xflex-col xmax-w-[600px] xpy-12">
        <!-- media -->
        <div class="image-block-wrapper">
          <img class="content-main">
          <video
            autoplay="autoplay"
            loop="loop"
            muted="muted"
            class="content-main xaspect-square"
            playsinline
          ></video>
          <div class="product-info-container xabsolute xbottom-0 xinset-x-0">
            <div class="product-info-wrapper xpx-10 xpb-8 xflex xitems-center xjustify-between xspace-x-4">
              <div class="product-text-wrapper xw-fit xpy-1 xpx-4 xrounded-2xl">
                <p class="product-title"></p>
                <p class="product-price"></p>
              </div>
              <img class="product-image xw-20 xmb-1 xanimate-spin-slow">
            </div>
          </div>
        </div>
        <!-- hashtags -->
        <div
          class="hashtags xmy-4 xtext-sm sm:xtext-[14px] sm:xleading-9"
        ></div>
        <!-- user details -->
        <div>
          <a target="_blank" class="user-profile-link xflex xspace-x-5 xw-fit">
            <img class="user-image xrounded-full xw-20 xh-20">
            <div>
              <p class="user-name"></p>
              <p class="user-handle"></p>
            </div>
          </a>
        </div>
      </div>
    </div>
  </div>
  <a
    href="/pages/our-social-lookbook-gumballpoodle"
    class="show-more"
    data-shift="5"
    >See All</a
  >
</div>

