    <svg
      class="control-arrow right-arrow"
      width="14"
      height="25"
      viewBox="0 0 14 25"
      fill="none"
      xmlns="http://www.w3.org/2000/svg"
    >
      <path d="M1.375 1.5L12.375 12.5L1.375 23.5" stroke="white" stroke-width="2.75" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  </div>

  <div class="lookbook-popup">
    <div class="slider">
      <div class="slide">
        <div class="image-block-wrapper">
          <img class="content-main">
          <img class="content-background">

          <video
            loop="loop"
            muted="muted"
            controls="true"
            class="content-main"
          ></video>
          <video preload="metadata" class="content-background"></video>
          <div class="video-switcher"></div>
        </div>

        <div class="info">
          <div class="product-info">
            <span class="title">Featured Product</span>
            <img class="product-image">
            <span class="product-name"></span>
            <span class="product-price"></span>
            <a href="#" class="buy-button">Buy Now</a>
          </div>

          <hr>

          <div class="user-info">
            <div class="personal-info">
              <a
                target="_blank"
                class="user-link"
                href=""
                ><img class="user-image"
              ></a>
              <span class="user-name"></span>
              <span class="user-date"></span>
              <i class="fa fa-instagram"></i>
            </div>
            <div class="text"></div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

